{
  "ignore": [
    ".idea",
    ".minio_storage",
    ".git",
    ".gitkeep",
    ".github",
    ".pgdata",
    ".tmp",
    ".vscode",
    "docs",
    "license",
    "stylesheets",
    ".editorconfig",
    ".prettierignore",
    ".prettierrc",
    "**/*.pyc",
    "**/*.gz",
    "**/*.mp3",
    "**/sqitch.plan",
    "**/build",
    "**/static",
    "**/tmp",
    "detpyvenv",
    ".DS_Store"
  ],
  "license": "license",
  "defaultFormat": {
    "eachLine": {
      "prepend": "# "
    }
  },
  "licenseFormats": {
    "gitignore|npmignore|eslintignore|dockerignore|sh|py|editorconfig|txt|yml|yaml": {
      "eachLine": {
        "prepend": "# "
      }
    },
    "html|xml|svg|md": {
      "prepend": "<!--",
      "append": "-->"
    },
    "js|ts|css|scss|less|php|as|c|java|cpp|go|cto|acl": {
      "prepend": "/*",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "sql": {
      "eachLine": {
        "prepend": "-- "
      }
    },
    "dotfile|env|dev|local|k8|example|env_example": {
      "eachLine": {
        "prepend": "# "
      }
    }
  }
}
